{
  "name": "ethagentic-server",
  "version": "0.0.1",
  "author": "ethagentic - team backend - development",
  "main": "src/main.js",
  "license": "private",
  "private": true,
  "description": "Eth Agentic - server for ethagentic project",
  "engines": {
    "node": ">=22.10.0",
    "yarn": ">=1.22.20",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "rimraf ./dist && tsc",
    "start2": "npm run build && node dist/main.js",
    "start": "cross-env NODE_ENV=local tsnd --respawn --ignore node_modules --transpile-only --project tsconfig.json -r tsconfig-paths/register ./src/main.ts",
    "local": "cross-env NODE_ENV=local tsnd --respawn --ignore node_modules --transpile-only --project tsconfig.json -r tsconfig-paths/register ./src/main.ts",
    "deps:check": "npx npm-check-updates",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --write src/**/*.ts",
    "infra:start": "docker-compose up -d --build",
    "infra:stop": "docker-compose down",
    "docker:production": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:staging": "docker-compose -f docker-compose.yml -f docker-compose.staging.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "prepare": "husky install"
  },
  "dependencies": {
    "ansi-colors": "4.1.3",
    "axios": "1.7.8",
    "bcryptjs": "2.4.3",
    "boxen": "4",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "compression": "1.7.5",
    "cross-env": "7.0.3",
    "dayjs": "1.11.13",
    "dotenv": "16.4.6",
    "env-var": "7.5.0",
    "express": "4.21.1",
    "express-rate-limit": "7.4.1",
    "helmet": "8.0.0",
    "hpp": "0.2.3",
    "http-errors": "2.0.0",
    "http-status-codes": "2.3.0",
    "multer": "1.4.5-lts.1",
    "reflect-metadata": "0.2.2",
    "socket.io": "4.8.1",
    "uuid": "^11.0.5",
    "winston": "3.17.0",
    "winston-daily-rotate-file": "5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.16.0",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "@types/node": "22.10.1",
    "@typescript-eslint/eslint-plugin": "8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-formatter-codeframe": "7.32.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.14.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "15.13.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "1.8.10",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.2",
    "typescript-eslint": "8.17.0",
    "typescript-transform-paths": "3.5.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
